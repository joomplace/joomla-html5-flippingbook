<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<application>html5flipbook</application>
    <threadsafe>true</threadsafe>
	<version>2.2.3</version>
	
	<!-- Configure java.util.logging -->
	<system-properties>
		<property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
	</system-properties>
 
    <static-files>
        <include path="/**" expiration="10s" />
        <include path="/**.png" expiration="1m" />
        <include path="/**.jpg" expiration="1m" />
        <include path="/**.css" expiration="1s" />
        <include path="/**.js"  expiration="1s" />
        <include path="/**.manifest" expiration="1s" />
        <exclude path="/**.php" />
        <exclude path="/**.inc" />
    </static-files>
    
    <resource-files>
        <include path="/**.php" />
        <include path="/**.inc" />
        <include path="/**.html" />
    </resource-files>
	
	
</appengine-web-app>